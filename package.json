    "bevent": "~0.0.2",
    "bfile": "~0.0.2",
    "bfilter": "~0.1.1",
    "bheep": "~0.0.1",
    "binet": "~0.1.0",
    "blgr": "~0.0.2",
    "blru": "~0.0.1",
    "blst": "~0.0.1",
    "bns": "~0.0.6",
    "bmutex": "~0.0.2",
    "bn.js": "~4.11.8",
    "bs32": "~0.0.1",
    "bsip": "~0.0.2",
    "bsock": "~0.0.2",
    "bsocks": "~0.0.2",
    "bstring": "~0.0.2",
    "btcp": "~0.0.2",
    "btrie": "~0.0.1",
    "bufio": "~0.0.3",
    "bupnp": "~0.0.2",
